    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "browser-sync": "^2.18.12",
    "browser-sync-webpack-plugin": "^1.2.0",
    "bs-fullscreen-message": "^1.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
